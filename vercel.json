{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.agadirlocalguide.com" }],
      "destination": "https://agadirlocalguide.com/$1",
      "permanent": true
    },
    {
      "source": "/:slug((?!.*\\.).+)",
      "destination": "/:slug.html",
      "permanent": true
    }
  ],
  "cleanUrls": false
}

